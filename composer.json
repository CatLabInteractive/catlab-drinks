{
    "name": "catlabinteractive/catlab-drinks",
    "description": "CatLab Drinks project",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Thijs Van der Schaeghe",
            "email": "info@catlab.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "appstract/laravel-opcache": "^4.0",
        "catlabinteractive/charon-laravel": "~1.8.0",
        "catlabinteractive/cursor-pagination": "^1.0.8",
        "catlabinteractive/laravel-catlab-accounts": "^4.0",
        "catlabinteractive/requirements": "^1.0.3",
        "doctrine/dbal": "^3.6",
        "endroid/qr-code": "^4.5.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.7",
        "kouz/laravel-airbrake": "^0.8.0",
        "laravel/framework": "^9.0.0",
        "laravel/passport": "^11.8.0",
        "laravel/ui": "^4.2",
        "paynl/omnipay-paynl": "~3.0.3",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": false,
            "endroid/installer": false,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:CatLabInteractive/omnipay-paynl.git"
        }
    ]
}
