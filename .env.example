APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=mysql-db
DB_PORT=3306
DB_DATABASE=myuser
DB_USERNAME=myuser
DB_PASSWORD=secret

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

AIRBRAKE_PROJECT_ID=
AIRBRAKE_PROJECT_KEY=
AIRBRAKE_HOST=

# Trusted Proxies Configuration
# Use '*' to trust all proxies (recommended for Heroku/cloud deployments)
# Use specific IPs/CIDRs for local development (e.g., '172.17.0.0/8' for Docker)
# Use comma-separated values for multiple proxies (e.g., '172.17.0.0/8,10.0.0.0/8')
TRUSTED_PROXIES=*

# Passport OAuth2 encryption keys (optional)
# If not set, keys will be auto-generated as files in storage/.
# For Heroku multi-dyno deployments, set these to ensure all dynos use the same keys.
# Generate keys with: php artisan passport:keys
# Then copy the contents of storage/oauth-private.key and storage/oauth-public.key here.
# PASSPORT_PRIVATE_KEY=
# PASSPORT_PUBLIC_KEY=

# Single sign on (you probably do not need this).
CATLAB_CLIENT_ID=
CATLAB_CLIENT_SECRET=
